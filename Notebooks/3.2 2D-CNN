plit.loc[df_split['cv_fold']=='train', 'merged_id'].to_list())\n","  test_merged_id = tuple(df_split.loc[df_split['cv_fold']=='test', 'merged_id'].to_list())\n","  valid_merged_id = tuple(df_split.loc[df_split['cv_fold']=='valid', 'merged_id'].to_list())  \n","\n","  df_power_meter_train = df_power_meter_rolling_flip.loc[:, df_power_meter_rolling_flip.columns.str.startswith(train_merged_id)]  \n","  df_power_meter_test = df_power_meter_rolling_flip.loc[:, df_power_meter_rolling_flip.columns.str.startswith(test_merged_id)]  \n","  df_power_meter_valid = df_power_meter_rolling_flip.loc[:, df_power_meter_rolling_flip.columns.str.startswith(valid_merged_id)] \n","\n","  keras.backend.clear_session()\n","\n","  # Prepare numpy array\n","  x_train, x_train_noisy, x_valid, x_valid_noisy, x_test, x_test_noisy = prepare_2d_array(df_power_meter_train, \n","                                                    df_power_meter_valid,\n","                                                    df_power_meter_test,\n","                                                    mask_mixed)\n","\n","  # Creat and train NN model\n","  NN_model = create_2D_CNN()\n","  NN_model.summary()\n","\n","  history = NN_model.fit(\n","      x_train_noisy,\n","      x_train,\n","      epochs=50,\n","      batch_size=128,\n","      #validation_split=0.20,\n","      validation_data=(x_valid_noisy, x_valid),\n","      #shuffle=False,\n","      callbacks=[\n","          keras.callbacks.EarlyStopping(monitor=\"val_loss\", patience=5, mode=\"min\")\n","      ],\n","      verbose=0\n","  )\n","\n","  histories.append(history)\n","\n","  # Display training history\n","  val_loss = history.history['val_loss'][-1]\n","  loss = history.history['loss'][-1]\n","\n","  plt.plot(history.history['loss'])\n","  plt.plot(history.history['val_loss'])\n","  plt.title('Model loss')\n","  plt.ylabel('Loss')\n","  plt.xlabel('Epoch')\n","  plt.legend(['Train', 'Valid'], loc='upper left')\n","  plt.show()\n","\n","  # Get test MSE loss\n","  x_test_pred = NN_model.predict(x_test_noisy)[:,:,0]\n","  test_mse_loss = np.mean((x_test_pred - x_test[:,:,0])**2, axis=1)\n","\n","  mask_missing_power_meter = x_test[:,:,0]!=x_test_noisy[:,:,0]\n","  test_mse_loss_missing = np.mean(((x_test_pred - x_test[:,:,0])**2)*mask_missing_power_meter) \n","\n","  plt.hist(test_mse_loss, bins=50)\n","  plt.xlabel(\"test MSE loss\")\n","  plt.ylabel(\"No of samples\")\n","  plt.show()\n","\n","  avg = np.mean(test_mse_loss)\n","  avg_missing = np.mean(test_mse_loss_missing)\n","  test_MSE.append(avg)\n","  test_MSE_missing.append(avg_missing)\n","\n","  print('train loss: '+str(round(loss, 4)))\n","  print('valid loss: '+str(round(val_loss, 4)))\n","  print(\"test loss: \"+str(round(avg, 4)))  \n","  print(\"test loss (missing): \"+str(round(avg_missing, 4))) \n","\n","  NN_model.save_weights('models/2D_CNN_cv'+str(int(cv_group_test)))\n"]},{"cell_type":"code","execution_count":21,"metadata":{"id":"y6RUtD3xD4N2","executionInfo":{"status":"ok","timestamp":1672295158267,"user_tz":-480,"elapsed":17,"user":{"displayName":"Patrick Fu","userId":"17527192923561102849"}}},"outputs":[],"source":[]}],"metadata":{"accelerator":"GPU","colab":{"machine_shape":"hm","provenance":[],"authorship_tag":"ABX9TyOXb69LMopkStIIs1TG2DjX"},"gpuClass":"premium","kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":0}