mask):\n","  \n","    train = df2array(energy_train)\n","    train_noisy = df2array(create_missing_data(energy_train, missing_mask))\n","\n","    valid = df2array(energy_valid) \n","    valid_noisy = df2array(create_missing_data(energy_valid, missing_mask)) \n","\n","    test = df2array(energy_test) \n","    test_noisy = df2array(create_missing_data(energy_test, missing_mask)) \n","\n","    return train, train_noisy, valid, valid_noisy, test, test_noisy "]},{"cell_type":"code","execution_count":20,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":17,"resources":{"http://localhost:8080/static/components/requirejs/require.js":{"data":"","headers":[["content-type","application/javascript"]],"ok":true,"status":200,"status_text":""}}},"executionInfo":{"elapsed":23,"status":"ok","timestamp":1669971608091,"user":{"displayName":"Patrick Fu","userId":"17527192923561102849"},"user_tz":-480},"id":"pUwqox2UCWr3","outputId":"61a35f98-491a-4834-c3a8-caec6763233c"},"outputs":[{"output_type":"display_data","data":{"text/plain":["<IPython.core.display.HTML object>"],"text/html":["\n","        <script src=\"/static/components/requirejs/require.js\"></script>\n","        <script>\n","          requirejs.config({\n","            paths: {\n","              base: '/static/base',\n","              plotly: 'https://cdn.plot.ly/plotly-1.5.1.min.js?noext',\n","            },\n","          });\n","        </script>\n","        "]},"metadata":{}}],"source":["def create_1D_CNN():\n","    # Build 1D CNN autoencoder\n","    model = keras.Sequential(\n","        [\n","            layers.Input(shape=(x_train.shape[1], x_train.shape[2])),\n","            layers.Conv1D(\n","                filters=32, kernel_size=7, padding=\"same\", strides=2, activation=\"LeakyReLU\"\n","            ),\n","            layers.Dropout(rate=0.2),\n","            layers.Conv1D(\n","                filters=16, kernel_size=7, padding=\"same\", strides=2, activation=\"LeakyReLU\"\n","            ),\n","            layers.Conv1DTranspose(\n","                filters=16, kernel_size=7, padding=\"same\", strides=2, activation=\"LeakyReLU\"\n","            ),\n","            layers.Dropout(rate=0.2),\n","            layers.Conv1DTranspose(\n","                filters=32, kernel_size=7, padding=\"same\", strides=2, activation=\"LeakyReLU\"\n","            ),\n","            layers.Conv1DTranspose(filters=1, kernel_size=7, padding=\"same\"),\n","        ]\n","    )\n","    model.compile(optimizer=keras.optimizers.Adam(learning_rate=0.001), loss=\"mse\")\n","\n","    return model"]},{"cell_type":"markdown","metadata":{"id":"vuptnBDhFvf4"},"source":["#### 3.2 Mixed missing"]},{"cell_type":"code","source":["for cv_group_test in np.arange(5):\n","  cv_group_valid = (cv_group_test+1)%5\n","  df_split['cv_fold'] = 'train'\n","  df_split.loc[df_split['cv_group']==cv_group_test, 'cv_fold'] = 'test'\n","  df_split.loc[df_split['cv_group']==cv_group_valid, 'cv_fold'] = 'valid'\n","\n","  train_merged_id = tuple(df_split.loc[df_split['cv_fold']=='train', 'merged_id'].to_list())\n","  test_merged_id = tuple(df_split.loc[df_split['cv_fold']=='test', 'merged_id'].to_list())\n","  valid_merged_id = tuple(df_split.loc[df_split['cv_fold']=='valid', 'merged_id'].to_list())  \n","\n","  df_power_meter_train = df_power_meter_rolling_flip.loc[:, df_power_meter_rolling_flip.columns.str.startswith(train_merged_id)]  \n","  df_power_meter_test = df_power_meter_rolling_flip.loc[:, df_power_meter_rolling_flip.columns.str.startswith(test_merged_id)]  \n","  df_power_meter_valid = df_power_meter_rolling_flip.loc[:, df_power_meter_rolling_flip.columns.str.startswith(valid_merged_id)] \n","\n","  keras.backend.clear_session()\n","\n","  # Prepare numpy array\n","  x_train, x_train_noisy, x_valid, x_valid_noisy, x_test, x_test_noisy = prepare_1d_array(df_power_meter_train, \n","                                                    df_power_meter_valid, \n","                                                    df_power_meter_test, \n","                                                    mask_mixed)\n","\n","  # Creat and train NN model\n","  NN_model = create_1D_CNN()\n","  NN_model.summary()\n","  \n","  history = NN_model.fit(\n","      x_train_noisy,\n","      x_train,\n","      epochs=50,\n","      batch_size=128,\n","      #validation_split=0.20,\n","      validation_data=(x_valid_noisy, x_valid),\n","      #shuffle=False,\n","      callbacks=[\n","          keras.callbacks.EarlyStopping(monitor=\"val_loss\", patience=5, mode=\"min\")\n","      ],\n","      verbose=0\n","  )\n","\n","  histories.append(history)\n","\n","  # Display training history\n","  val_loss = history.history['val_loss'][-1]\n","  loss = history.history['loss'][-1]\n","\n","  plt.plot(history.history['loss'])\n","  plt.plot(history.history['val_loss'])\n","  plt.title('Model loss')\n","  plt.ylabel('Loss')\n","  plt.xlabel('Epoch')\n","  plt.legend(['Train', 'Valid'], loc='upper left')\n","  plt.show()\n","\n","  # Get test MSE loss\n","  x_test_pred = NN_model.predict(x_test_noisy)[:,:,0]\n","  test_mse_loss = np.mean((x_test_pred - x_test[:,:,0])**2, axis=1)\n","\n","  mask_missing_power_meter = x_test[:,:,0]!=x_test_noisy[:,:,0]\n","  test_mse_loss_missing = np.mean(((x_test_pred - x_test[:,:,0])**2)*mask_missing_power_meter) \n","\n","  plt.hist(test_mse_loss, bins=50)\n","  plt.xlabel(\"test MSE loss\")\n","  plt.ylabel(\"No of samples\")\n","  plt.show()\n","\n","  avg = np.mean(test_mse_loss)\n","  avg_missing = np.mean(test_mse_loss_missing)\n","  test_MSE.append(avg)\n","  test_MSE_missing.append(avg_missing)\n","\n","  print('train loss: '+str(round(loss, 4)))\n","  print('valid loss: '+str(round(val_loss, 4)))\n","  print(\"test loss: \"+str(round(avg, 4)))  \n","  print(\"test loss (missing): \"+str(round(avg_missing, 4)))   \n","\n","  NN_model.save_weights('models/1D_CNN_cv'+str(int(cv_group_test)))\n"],"metadata":{"colab":{"resources":{"http://localhost:8080/static/components